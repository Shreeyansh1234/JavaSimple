package Practice;

import org.openqa.selenium.WebDriver;

public class Ellicium {

	public static void main(String[] args) {
		int array[]= {14,5,2,2,3};
		for (int i = 0; i < array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] > array[j]) {
					int k = array[i];
					array[i] = array[j];
					array[j] = k;
				}
			}
	}

}
